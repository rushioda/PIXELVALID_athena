       }

       dir Jets {
 	output = HLT/TRHLT/Jets
        hist HLT.*P[ST] {
	  regex = 1
          algorithm = HLT_Histogram_Not_Empty&GatherData
##	  algorithm = HLT_PassInput
	}
	hist HLT_JetsRAW {
	  regex = 1
          algorithm = HLT_Histogram_Not_Empty&GatherData
