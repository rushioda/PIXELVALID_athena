######################################## THRESHOLDS #################################################
thresholds HLTjet_CheckMeanThresh {
  limits chi2 {
    warning = 300
    error = 350
  }
}

thresholds HLTjet_KolmogorovThresh {
  limits MaxDist {
    warning = 0.05
    error = 0.15
  }
}

thresholds HLTjet_NBinsThresh {
  limits NBins {
    warning = 1
    error = 1
  }
}

