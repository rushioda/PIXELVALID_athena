############################## HLT ###################################

dir HLT {
  dir JetMon {
    dir HLT {
      dir a4tcemjesPS {
        hist .* {
          regex = 1
          algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
          output = HLT/TRJET/EXPERT/HLT/a4tcemjesPS
      	}
      } ## End a4tcemjesPS
    } ## End HLT
  } ##End JetMon
} ##End HLT

