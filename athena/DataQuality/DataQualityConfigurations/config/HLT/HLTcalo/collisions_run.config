	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_num_map {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData
	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_et {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData
	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_eta {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData
	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_phi {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData
	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_size {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData
	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_vs_OFF_et_vs_et {
        algorithm = HLTCaloHistogram_Not_Empty&GatherData
	output = HLT/TRCAL/Shifter/xAODTrigEMClusters
      }
      hist HLT_vs_OFF_resolution {
