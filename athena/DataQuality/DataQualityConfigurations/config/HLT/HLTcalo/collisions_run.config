        algorithm = GatherData
        output = HLT/TRCAL/Expert/CaloCells
      }
      hist HLT_EMB__num_map {
        algorithm = GatherData
        output = HLT/TRCAL/Expert/CaloCells
      } 
      hist HLT_EMB__energy {
        algorithm = GatherData
        output = HLT/TRCAL/Expert/CaloCells
      }
