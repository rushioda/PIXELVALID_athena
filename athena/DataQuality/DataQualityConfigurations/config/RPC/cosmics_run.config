#############

output top_level {
  algorithm = WorstCaseSummary  

  output MuonDetectors {
    

  output L1MU {
    algorithm = WorstCaseSummary  
    output L1MUB {
     algorithm = WorstCaseSummary 
       output Expert {
        algorithm = WorstCaseSummary 
        set_weight = 0.0
        weight = 0.0
        output GLOBAL {
         algorithm = WorstCaseSummary     
        }
        output PAD {
         algorithm = WorstCaseSummary     
        } 
        output Pt_Thr {
         algorithm = WorstCaseSummary     
        }
        output TurnOnCurve {
         algorithm = WorstCaseSummary 
         output PhiEtaCoin {
          algorithm = WorstCaseSummary     
         } 
         output PAD {
          algorithm = WorstCaseSummary     
         } 
         output MUCTPI {
          algorithm = WorstCaseSummary     
         }   
        }
      }
       output Shifter {
        algorithm = WorstCaseSummary 
        output PADBA {
         algorithm = WorstCaseSummary  
         output Coverage {
          algorithm = WorstCaseSummary    
         }    
         output PlotVsLB {
          algorithm = WorstCaseSummary    
         } 
        }
        output PADBC {
         algorithm = WorstCaseSummary  
         output Coverage {
          algorithm = WorstCaseSummary    
         }    
         output PlotVsLB {
          algorithm = WorstCaseSummary    
         }    
        }
      }
    }
  }

    output RPC {
      algorithm = WorstCaseSummary 
      output Shifter {
       algorithm = WorstCaseSummary 
	output GLOBAL {
	  algorithm = WorstCaseSummary  
        }             
        output RPCBA {
	  algorithm = WorstCaseSummary
	  output PlotVsLB {
	    algorithm = WorstCaseSummary  
          } 
	  output Coverage {
	    algorithm = WorstCaseSummary  
          }  
        }
        output RPCBC {
	  algorithm = WorstCaseSummary 
	  output PlotVsLB {
	    algorithm = WorstCaseSummary  
          } 
	  output Coverage {
	    algorithm = WorstCaseSummary  
          }  
        }
      } 
      output Expert {    
	set_weight = 0.0
	weight = 0.0
	output GLOBAL {
	  algorithm = WorstCaseSummary  
        } 
	output Efficiency_per_Sector {    
	  algorithm = WorstCaseSummary  
	}
	output Coverage {    
	  algorithm = WorstCaseSummary  
	}
	output ClusterSize_per_Sector {    
	  algorithm = WorstCaseSummary  
	}
	output Noise_per_Sector {    
	  algorithm = WorstCaseSummary  
	}
	output RPC_StandAlone_TrackQuality {    
	  algorithm = WorstCaseSummary  
	}  
	output Time_per_Sector {
	    algorithm = WorstCaseSummary  
        }            
        output RPCBA {
	  algorithm = WorstCaseSummary
	  output PlotVsLB {
	    algorithm = WorstCaseSummary  
          } 
	  output Coverage {
	    algorithm = WorstCaseSummary  
          }  
        }            
        output RPCBC {
	  algorithm = WorstCaseSummary
	  output PlotVsLB {
	    algorithm = WorstCaseSummary  
          } 
	  output Coverage {
	    algorithm = WorstCaseSummary  
          }  
        }
      }
    }				

  } #MuonDetectors
     
}

