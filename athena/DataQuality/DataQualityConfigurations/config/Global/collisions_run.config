             hist tileCellEneEtaPhiSampDAnyPhysTrig@HotSpotFinder {
                 output = Global/HotSpotFinder/Detectors/Tile
                 algorithm = LAr_CaloCells_BinsDiffFromStripMedian
             }
             hist tileCellEneEtaPhiSampEAnyPhysTrig@HotSpotFinder {
                 output = Global/HotSpotFinder/Detectors/Tile
                 algorithm = LAr_CaloCells_BinsDiffFromStripMedian
             }
         }
     }
}
         

