#############
# Output
#############

output top_level {
 # notice NO ALGORITHM specified here
 output CaloMonitoring {
#    output CaloCellsForExpert {
#    } 
  output CaloMonExpert {
    
    output CaloCalTopoCluster { 
       output ClustersForExpert {
          weight = 0. # The result is not propagated to global flag
          output 2dOccupancy {
          }
          output 2dAvEnergy {
          }
          output 1dOccupancy {
          }
          output 1dAvEnergy {
          }
          output LeadCell {
          }

          output TimeEnergy {
          }
          output General {
          }
          output TileHistograms {
          }
          output TotalEnergy {
          }
          output TransEnergy {
          }
       }
    }
    output EMTopoCluster {
       output ClustersForExpert {
       weight = 0. # The result is not propagated to global flag
          output 2dOccupancy {
          }
          output 2dAvEnergy {
          }
          output 1dOccupancy {
          }
          output 1dAvEnergy {
          }
          output LeadCell {
          }

          output TimeEnergy {
          }
          output General {
          }
          output TotalEnergy {
          }
          output TransEnergy {
          }

       }
    }
  }
  output CaloMonShift {
       algorithm = WorstCaseYellowCaloMon
       output CaloMonBAR {
          algorithm = WorstCaseYellowCaloMon
          output CaloTopoClustersBAR {
           weight = 1.0
           algorithm = WorstCaseYellowCaloMon

          }
          output EMTopoClustersBAR {
           weight = 1.0
           algorithm = WorstCaseYellowCaloMon

          }
          
       }
       output CaloMonECA {
          algorithm = WorstCaseYellowCaloMon
          output CaloTopoClustersECA {
           weight = 1.0
           algorithm = WorstCaseYellowCaloMon

          }
          output EMTopoClustersECA {
           weight = 1.0
           algorithm = WorstCaseYellowCaloMon

          }
          
       }
       output CaloMonECC {
          algorithm = WorstCaseYellowCaloMon
          output CaloTopoClustersECC {
           weight = 1.0
           algorithm = WorstCaseYellowCaloMon

          }
          output EMTopoClustersECC {
           weight = 1.0
           algorithm = WorstCaseYellowCaloMon

          }
          
       }
    }
  }
# output your top level {
# # put your output tree here
# }
}

