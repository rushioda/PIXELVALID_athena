#############
# Algorithms
#############

algorithm Muon_BinContentComp {
  libname = libdqm_algorithms.so
  name = BinContentComp
  reference = AllShiftHistsRef
  thresholds = Muon_thBinContentComp
  NSigma = 5
  Ignore0 = 10
}
algorithm Muon_Histogram_Not_Empty {
  libname = libdqm_algorithms.so
  name = Histogram_Not_Empty
  reference = AllShiftHistsRef
}
algorithm Muon_NumHitsPerTrack_CheckMean {
  libname = libdqm_algorithms.so
  name = CheckHisto_Mean
  reference = AllShiftHistsRef
  thresholds = Muon_NumHitsPerTrack_thMean

  algorithm MDT { 
    thresholds = Muon_NumMdtHitsPerTrack_thMean
  }
  algorithm RPC { 
    thresholds = Muon_NumRpcHitsPerTrack_thMean
  }
  algorithm TGC { 
    thresholds = Muon_NumTgcHitsPerTrack_thMean
  }
  algorithm CSC { 
    thresholds = Muon_NumCscHitsPerTrack_thMean
  }

}
algorithm Muon_NumTracks_CheckMean {
  libname = libdqm_algorithms.so
  name = CheckHisto_Mean
  reference = AllShiftHistsRef
  thresholds = Muon_NumTracks_thMean
}
algorithm MuonSegments_Histogram_Not_Empty {
  libname = libdqm_algorithms.so
  name = Histogram_Not_Empty
  reference = AllShiftHistsRef
}

algorithm MuonDet_AlgNoEmpty {
  libname = libdqm_algorithms.so
  name = Histogram_Not_Empty
}

algorithm RPC_bin {
  libname = libdqm_algorithms.so
  name = BinContentComp
  reference = AllShiftHistsRef
  NSigma = 1
  thresholds = RPC_th1
}

algorithm MuonDet_Chi2Prob {
  libname = libdqm_algorithms.so
  name = Chi2Test_Prob
  reference = AllShiftHistsRef
  thresholds = MuonDet_Chi2ProbThresh
}

algorithm RPCLV1_SL_GreaterThan_Threshold {
  libname = libDataQualityInterfacesAlgs.so
  name = CountsBinsGreaterThan
  BinStart = 1
  BinEnd   = 64
  CountsTh = 10
  thresholds = RPCLV1_thSL      
}

algorithm RPCLV1_PAD_GreaterThan_Threshold {
  libname = libDataQualityInterfacesAlgs.so
  name = CountsBinsGreaterThan
  BinStart = 1
  BinEnd   = 64
  CountsTh = 10
  thresholds = RPCLV1_thPAD     
}

algorithm RPCLV1_CM_GreaterThan_Threshold {
  libname = libDataQualityInterfacesAlgs.so
  name = CountsBinsGreaterThan
  BinStart = 1
  BinEnd   = 64
  CountsTh = 10
  thresholds = RPCLV1_thCM    
}

algorithm RPCLV1_ROI_LowPt_GreaterThan_Threshold {
  libname = libDataQualityInterfacesAlgs.so
  name = CountsBinsGreaterThan
  BinStart = 1
  BinEnd   = 64
  CountsTh = 10
  thresholds = RPCLV1_thROILowPt    
}

algorithm RPCLV1_ROI_HighPt_GreaterThan_Threshold {
  libname = libDataQualityInterfacesAlgs.so
  name = CountsBinsGreaterThan
  BinStart = 1
  BinEnd   = 64
  CountsTh = 1
  thresholds = RPCLV1_thROIHighPt    
}

algorithm MuonDet_CheckMean {
  libname = libdqm_algorithms.so
  name = CheckHisto_Mean
  thresholds = MuonDet_thMean   
}

algorithm RPC_Checkchi2 {
  libname = libdqm_algorithms.so
  reference = AllShiftHistsRef
  name = CheckHisto_Mean
  thresholds = RPC_thchi2   
}

algorithm MuonDet_BinDump {
  libname = libDataQualityInterfacesAlgs.so
  name = CountsBinsGreaterThan
  BinStart = 1
  BinEnd   = 64
  Method   = 0
  CountsTh = 10
  thresholds = MuonDet_thNEntries   
}
algorithm MDTvsRPC_CorrelationStripvsTube {
  libname = libDataQualityInterfacesAlgs.so
  name = CorrelationYX
  BinStart   = 2
  BinEnd     = 1
  thresholds = MDTvsRPC_thCorrMin   
}
## TGC

algorithm TGC_Wire_Hits {
  libname = libdqm_algorithms.so
  name = CheckHisto_Mean
  thresholds = TGC_MeanOfWireHits
}

algorithm TGC_Strip_Hits {
  libname = libdqm_algorithms.so
  name = CheckHisto_Mean
  thresholds = TGC_MeanOfStripHits
}

algorithm TGC_Occupancy {
  libname = libdqm_algorithms.so
  name = Bins_GreaterThan_Threshold
  BinThreshold = 1.0
  thresholds = TGC_OccupancyThreshold
}

algorithm TGC_Efficiency {
  libname = libdqm_algorithms.so
  name = Bins_LessThan_Threshold
  BinThreshold = 0.95
  thresholds = TGC_EfficiencyThreshold
}

## TGCLV1

algorithm TGC_Hit_Timing {
  libname =  libdqm_algorithms.so
  name = SideBand_Relative
  thresholds = TGC_HitTimingThreshold
  UseUnderFlow = 0
  UseOverFlow = 0
  Min = 1
  Max = 1.99
}

algorithm TGC_Trigger_Timing {
  libname =  libdqm_algorithms.so
  name = SideBand_Relative
  thresholds = TGC_TriggerTimingThreshold
  UseUnderFlow = 0
  UseOverFlow = 0
  Min = 1
  Max = 1.99
}

algorithm TGC_LowPt {
