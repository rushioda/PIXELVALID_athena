<!-- A-SIDE -->
<composition name="NSW" >

  <!-- LARGE SECTOR -->
  <!-- sTGC multilayer 1 -->
  <mposPhi volume="sTG1-1-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(NSW_sTGC_FrontDisk_bottomPlateLength/2.);NSW_zPos+(2.*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap-(NSW_sTGC_Tck/2.)" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sTG1-2-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+NSW_sTGC_FrontDisk_bottomPlateLengthNoSteps+(NSW_sTGC_FrontDisk_secondPlateLength/2.)+NSW_sTGC_SpacerLength;NSW_zPos+(2.*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap-(NSW_sTGC_Tck/2.)" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sTG1-3-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+NSW_sTGC_FrontDisk_bottomPlateLengthNoSteps+NSW_sTGC_FrontDisk_secondPlateLength+(NSW_sTGC_FrontDisk_standardPlateLength/2.)+(2.*NSW_sTGC_SpacerLength);NSW_zPos+(2.*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap-(NSW_sTGC_Tck/2.)" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sTG1-4-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+NSW_sTGC_FrontDisk_bottomPlateLengthNoSteps+NSW_sTGC_FrontDisk_secondPlateLength+(1.5*NSW_sTGC_FrontDisk_standardPlateLength)+(3.*NSW_sTGC_SpacerLength);NSW_zPos+(2.*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap-(NSW_sTGC_Tck/2.)" Phi0="0." rot=" 90.; 270.; 90." />
  <!-- MM multilayer 1 -->
  <mposPhi volume="sMD1-1-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(NSW_MM_FrontDisk_standardPlateLength/2.);NSW_zPos+(2.5*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sMD1-2-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(1.5*NSW_MM_FrontDisk_standardPlateLength)+NSW_MM_SpacerLength;NSW_zPos+(2.5*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sMD1-3-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(2.5*NSW_MM_FrontDisk_standardPlateLength)+(2.*NSW_MM_SpacerLength);NSW_zPos+(2.5*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sMD1-4-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(3.*NSW_MM_FrontDisk_standardPlateLength)+(NSW_MM_FrontDisk_topPlateLength/2.)+(3.*NSW_MM_SpacerLength);NSW_zPos+(2.5*NSW_MM_Tck)+NSW_MM_SpacerThickness+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <!-- spacer -->
  <mposPhi volume="spa1-1" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(NSW_MM_FrontDisk_standardPlateLength/2.);NSW_zPos+(3.*NSW_MM_Tck)+(1.5*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="spa1-2" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(1.5*NSW_MM_FrontDisk_standardPlateLength)+NSW_MM_SpacerLength;NSW_zPos+(3.*NSW_MM_Tck)+(1.5*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="spa1-3" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(2.5*NSW_MM_FrontDisk_standardPlateLength)+(2.*NSW_MM_SpacerLength);NSW_zPos+(3.*NSW_MM_Tck)+(1.5*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="spa1-4" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(3.*NSW_MM_FrontDisk_standardPlateLength)+(NSW_MM_FrontDisk_topPlateLength/2.)+(3.*NSW_MM_SpacerLength);NSW_zPos+(3.*NSW_MM_Tck)+(1.5*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <!-- MM multilayer 2 -->
  <mposPhi volume="sMD1-1-2" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(NSW_MM_FrontDisk_standardPlateLength/2.);NSW_zPos+(3.5*NSW_MM_Tck)+(2.*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sMD1-2-2" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(1.5*NSW_MM_FrontDisk_standardPlateLength)+NSW_MM_SpacerLength;NSW_zPos+(3.5*NSW_MM_Tck)+(2.*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sMD1-3-2" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(2.5*NSW_MM_FrontDisk_standardPlateLength)+(2.*NSW_MM_SpacerLength);NSW_zPos+(3.5*NSW_MM_Tck)+(2.*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <mposPhi volume="sMD1-4-2" ncopy="8" R_Z="NSW_MM_FrontDisk_radius+(3.*NSW_MM_FrontDisk_standardPlateLength)+(NSW_MM_FrontDisk_topPlateLength/2.)+(3.*NSW_MM_SpacerLength);NSW_zPos+(3.5*NSW_MM_Tck)+(2.*NSW_MM_SpacerThickness)+NSW_SectorGap" Phi0="0." rot=" 90.; 270.; 90." />
  <!-- sTGC multilayer 2 -->
