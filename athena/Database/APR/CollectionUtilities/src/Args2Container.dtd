<!ELEMENT ArgList (ToolInfo+)>
<!ELEMENT ToolInfo (CliArg*)>
<!ATTLIST ToolInfo
	date    	CDATA	#IMPLIED> 
      	toolID    	ID   	#REQUIRED>
<!ELEMENT CliArg (datum*)>
<!ATTLIST CliArg
	name 	ID	#REQUIRED>
	option		CDATA	#REQUIRED>
<!ELEMENT datum ANY> 
