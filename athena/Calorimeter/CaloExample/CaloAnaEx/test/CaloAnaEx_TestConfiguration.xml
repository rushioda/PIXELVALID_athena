<jobGroups>
        
     <jobGroup name="CaloAnaExESDAOD" parent="RTT:Athena">
               <keepFilePattern>*.root</keepFilePattern>
               <keepFilePattern>*.data</keepFilePattern>
               <keepFilePattern>*.hist</keepFilePattern>
               <keepFilePattern>*.ps</keepFilePattern>
               <keepFilePattern>*.txt</keepFilePattern>
               <keepFilePattern>*.gif</keepFilePattern>
               <keepFilePattern>*.html</keepFilePattern>
               <keepFilePattern>*.log</keepFilePattern>
               <keepFilePattern>CaloAnaEx.css</keepFilePattern>
               <keepFilePattern displayColor="green">checks.html</keepFilePattern>
           
               <auxFilePattern>CaloAnaEx_runMacro*.C</auxFilePattern>
               <auxFilePattern>CaloAnaEx_runPython2*.C</auxFilePattern>
               <auxFilePattern>CaloAnaEx.css</auxFilePattern>
               <auxFilePattern>CaloAnaEx_HistoComparison.txt</auxFilePattern>
               <auxFilePattern>CaloAnaEx_check.py</auxFilePattern>
               <auxFilePattern>CaloAnaEx_HistoComparison.py</auxFilePattern>
               <auxFilePattern>CaloAnaEx_Truth.py</auxFilePattern>
               <auxFilePattern>CaloAnaEx_memleak.py</auxFilePattern>
               <auxFilePattern>CaloAnaEx_checkAODESDcontent.py</auxFilePattern>
               <auxFilePattern>CaloAnaEx_extractRec.py</auxFilePattern>

               <auxFilePattern>*Checks.py</auxFilePattern>

   <action>
            <modulename>RttLibraryTools</modulename>
            <testname>ROOTMacroRunner</testname>
            <arg>
               <argname>macro</argname>
               <argvalue>CaloAnaEx_runMacros2A.C</argvalue>
            </arg>
     </action>
                         
   <action> 
            <modulename>RttLibraryTools</modulename> 
            <testname>FileGrepper</testname> 

            <arg>
               <argname>inputFile</argname> 
               <argvalue>*_log</argvalue> 
            </arg>

            <arg> 
               <argname>outputFile</argname> 
               <argvalue>logExtract.txt</argvalue> 
            </arg> 

            <listarg> 
               <listargname>searchStrings</listargname> 
               <listargvalue>ChronoStatSvc                              INFO Time User   : Tot=</listargvalue>
               <listargvalue>FPEAuditor           INFO </listargvalue>
               <listargvalue>Py:PerfMonSvc        INFO </listargvalue>
             </listarg> 

      </action>
          
   </jobGroup>

</jobGroups>

