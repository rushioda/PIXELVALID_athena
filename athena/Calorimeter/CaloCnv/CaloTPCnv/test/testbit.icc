#ifndef CALOTPCNV_TESTBIT_ICC
#define CALOTPCNV_TESTBIT_ICC


namespace {

inline
bool testbit (unsigned int x, unsigned int i)
{
  return (x & (1U << i)) != 0;
}

} // anonymous namespace


#endif // not CALOTPCNV_TESTBIT_ICC
